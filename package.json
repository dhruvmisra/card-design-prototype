{
  "name": "vue-event-card",
  "version": "0.1.4-1",
  "description": "A Vue.js interactive card design component to show essentials and description for an event.",
  "private": false,
  "license": "MIT",
  "author": "Dhruv Misra",
  "main": "./dist/vue-event-card.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-event-card ./src/components/index.js",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-service": "^5.0.8",
    "gh-pages": "^2.1.1",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruvmisra/vue-event-card.git"
  },
  "keywords": [
    "vue",
    "vue event",
    "vue card",
    "vue event card",
    "card",
    "event",
    "card design",
    "interactive design"
  ]
}
